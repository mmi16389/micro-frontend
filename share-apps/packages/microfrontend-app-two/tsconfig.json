{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
        "esnext",
        "es2017",
        "es2015",
        "dom",
        "dom.iterable",
        "scripthost"
    ],
    "types": ["vuetify"],
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
